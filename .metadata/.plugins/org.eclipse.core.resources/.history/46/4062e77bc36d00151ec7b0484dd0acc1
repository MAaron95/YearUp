
public class Main {

	public static void main(String[] args) 
	{
		PersonEncaps student = new PersonEncaps("Mark Aaron", 19, "5551234567", "730 Peachtree St");
		System.out.println("Student's name is " + student.GetName());
		System.out.println("Student's age is " + student.GetAge());
		System.out.println("Student's phone number is " + student.GetPhoneNumber());
		System.out.println("Student's address is " + student.GetAddress());
		System.out.println("It is November 3, 2015 today");
		student.GrowsOlder();
		System.out.println("Student's age is now " + student.GetAge());
		
	}

}
